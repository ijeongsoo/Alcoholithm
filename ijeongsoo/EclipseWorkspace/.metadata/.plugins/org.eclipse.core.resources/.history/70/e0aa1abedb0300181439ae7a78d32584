package Day004DivideAndConquer;

import java.util.Scanner;

public class Compression {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int input = sc.nextInt();
		int[][] inputArr = new int[input][];
		for(int i=0; i<input; i++){
			for(int j=0; j<input; j++){
				inputArr[i][j]=sc.nextInt();
			}
		}
		
		System.out.println(comp(inputArr, input));
	}
	
	public static String comp(int[][] arr,int n, int s, int e, int turm){
		if(n==1){
	        return arr[s][e]+"";
		}
		
		
		return "";
	}
	
	public static String merge(int [][]arr, int s, int center, int e){
		
	}

}
